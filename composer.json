{
  "name": "tomekstach/sct-backend",
  "version": "1.0.0-alpha.1",
  "description": "Application code for sct.com",
  "homepage": "http://sct.com",
  "license": "MIT",
  "require": {
    "php": ">=7.2.5",
    "ext-pdo": "*",
    "joomla/application": "~2.0@dev",
    "joomla/controller": "~2.0@dev",
    "joomla/event": "~2.0@dev",
    "joomla/database": "~2.0@dev",
    "joomla/di": "~2.0@dev",
    "joomla/filesystem": "~2.0@dev",
    "joomla/model": "~2.0@dev",
    "joomla/registry": "~2.0@dev",
    "joomla/renderer": "~2.0@dev",
    "joomla/router": "~2.0@dev",
    "joomla/uri": "~2.0@dev",
    "joomla/view": "~2.0@dev",
    "joomla/session": "~2.0@dev",
    "joomla/authentication": "~2.0@dev",
    "joomla/language": "~2.0@dev",
    "ramsey/uuid": "~3.9",
    "symfony/asset": "5.0.*",
    "symfony/yaml": "5.0.*",
    "phpmailer/phpmailer": ">=6.1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "sct\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.2.24"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php -r \"copy('etc/config.dist.json', 'etc/config.json');\""
    ]
  }
}